{
  "name": "@mero/torrent-cli",
  "description": "Torrent CLI to create and share private files on LAN",
  "version": "0.0.1",
  "author": "Rafael Mello <merorafael@gmail.com>",
  "bin": {
    "torrent-cli": "./bin/run"
  },
  "bugs": "https://github.com/merorafael/torrent-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "axios": "^0.19.0",
    "create-torrent": "^3.33.0",
    "dat-swarm-defaults": "^1.0.2",
    "discovery-swarm": "^6.0.0",
    "humanize-duration": "^3.18.0",
    "listr": "^0.14.3",
    "webtorrent": "^0.103.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/merorafael/torrent-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "torrent-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
